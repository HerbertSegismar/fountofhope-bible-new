    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "backgroundColor": "#1e40af"
    },
    "web": {
      "favicon": "./assets/fountofhope-512x512.png"
    },
    "plugins": ["expo-sqlite", "expo-asset", "expo-font"],
    "extra": {
      "eas": {
        "projectId": "8655600c-f728-4a18-b204-41aeff1a964c"
